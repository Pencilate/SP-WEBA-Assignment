    <span class="navbar-toggler-icon"></span>
   </button>
   <div class="collapse navbar-collapse" id="navbarSupportedContent">
 

    @await Html.PartialAsync("_LoginPartial")




   </div>
  </div>
 </div>


 <div class="container body-content">

  @RenderBody()

 </div>


 <footer class="footer">
  <div class="container">
   <p class="text-muted">Remember to self-explore the _Layout.cshtml to see how the HTML markups are used to make the whole site consistent</p>
  </div>
